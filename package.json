{
  "name": "headzooio",
  "version": "1.0.0",
  "description": "headzoo.io",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "jest": {
    "cacheDirectory": "./.jest-cache"
  },
  "scripts": {
    "test": "jest",
    "watch": "encore dev --watch",
    "server": "php bin/console gos:websocket:server",
    "build": "encore production",
    "migrate": "php bin/console doctrine:migrations:migrate --no-interaction",
    "deploy": "ssh ubuntu@upnext.fm 'cd /var/www/upnext.fm && npm run pull'",
    "deploy:assets": "ssh ubuntu@upnext.fm 'cd /var/www/upnext.fm && npm install && composer update && bin/console cache:clear --no-warmup'",
    "deploy:params": "scp app/config/parameters.yml ubuntu@upnext.fm:/var/www/upnext.fm/app/config/parameters.yml && ssh ubuntu@upnext.fm 'cd /var/www/upnext.fm && bin/console cache:clear --no-warmup'",
    "deploy:db": "bin/dbexport.sh",
    "deploy:full": "npm run deploy && npm run deploy:assets && npm run deploy:params && npm run deploy:db",
    "pull": "git pull && npm run build && bin/console cache:clear --no-warmup",
    "lint": "eslint ./web/src/js --cache --cache-location=.cache/eslint --ext .js,.jsx",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "favico.js": "^0.3.10",
    "linkify-it": "^2.0.3",
    "material-ui": "1.0.0-beta.6",
    "materialize-css": "^0.100.1",
    "moment": "^2.18.1",
    "promise-polyfill": "6.0.2",
    "prop-types": "^15.5.10",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
    "react-color": "^2.13.8",
    "react-custom-scrollbars": "^4.1.2",
    "react-dom": "^15.6.1",
    "react-dropzone": "^4.1.0",
    "react-moment": "^0.6.4",
    "react-redux": "^5.0.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-share": "^1.15.1",
    "react-youtube": "^7.4.0",
    "redux": "^3.7.0",
    "redux-devtools": "^3.4.0",
    "redux-form": "^7.0.4",
    "redux-thunk": "^2.2.0",
    "tlds": "^1.194.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@symfony/webpack-encore": "^0.12.0",
    "autoprefixer": "^7.1.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-livereload-plugin": "^0.11.0"
  }
}
